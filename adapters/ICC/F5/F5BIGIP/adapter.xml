
<adapter name="F5BIGIP">
	<description>F5 (multi-config) load-balancers, Big-IP series, OS version 10.1.x.</description>
	<version>1.0.0</version>
	<services>
		<service name="CLICommon">
			<item type="common">F5_Common_CLI.xml</item>
		</service>

		<service name="CleanupParser">
			<item type="parser_definition">F5_Cleanup_Parser.xml</item>
			<item type="perl_script">F5_Cleanup_Parser_Script.pl</item>
		</service>

		<service name="ConfigBackup">
			<item type="builder_definition">F5_Config_Backup_Builder.xml</item>
			<item type="tcl_script">F5_Config_Backup_Builder_Script.xml</item>
		</service>

		<service name="ConfigDeploy">
			<item type="builder_definition">F5_Config_Deploy_Builder.xml</item>
			<item type="tcl_script">F5_Config_Deploy_Builder_Script.xml</item>
		</service>

		<service name="ImageDeploy">
			<item type="builder_definition">F5_Image_Deploy_Builder.xml</item>
			<item type="tcl_script">F5_Image_Deploy_Builder_Script.xml</item>
		</service>

	</services>
</adapter>

