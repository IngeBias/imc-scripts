<?xml version="1.0"?>

<common>
    <mode name="initialize" method="cli">
        <require-mode>connect</require-mode>
        <enter>
            initialize.tcl
        </enter>
    </mode>

    <mode name="exec" method="cli">
        <error>Failed to get to exec mode. </error>
        <require-mode>initialize</require-mode>
        <enter>
            enter_exec.tcl
        </enter>
        <exit>
            exit_exec.tcl
        </exit>
    </mode>

    <mode name="tmsh" method="cli">
        <error>Failed to get to enable mode. </error>
        <require-mode>exec</require-mode>
        <enter>
            enter_tmsh.tcl
        </enter>
        <exit>
            exit_tmsh.tcl
        </exit>
    </mode>

	<function name="ftp_trans_file" method="CLI">
        <error>An error occurred while trying to transfer file.</error>
        <enter>
            ftp_trans_file.tcl
        </enter>
    </function>

</common>
