<?xml version="1.0"?>
<!-- deploy image by SNMP -->

<!--
%index:     random generate index used for snmp set operation.
data-type:  INTEGER/Integer32/IpAddress/DisplayString/ObjectIdentifier/RowStatus
            default is INTEGER                
oper-type:  SET/GET/GETNEXT , default is SET   
            SET,     snmp set operation;
            GET,     snmp get operation;
            GETNEXT, snmp getnext operation.        
func-type:  Common/CheckResult/CheckExecResult, default is Oper
            Common, commit snmp operation;
            CheckResult, check if the snmp operation result is success;
            CheckExecResult, check is the snmp exection result is success.
-->

<definition>
    <func-type>Common</step-type>
    <oper-type>SNMP_SET</oper-type>
    <error>Failed to deploy image file to device by snmp, commit snmp failed.</error>
    
	<vb-list>	
		<vb>
			<oid>1.3.6.1.4.1.522.3.4.1.0</oid>
			<value>$TFTPServer</value>
			<data-type>IpAddress</data-type>		
		</vb>
		
		<vb>
			<oid>1.3.6.1.4.1.522.3.4.2.0</oid>
			<value>2</value>		
			</vb>
		
		<vb>
			<oid>1.3.6.1.4.1.522.3.4.9.0</oid>
			<value>$TFTPFile</value> 
			<data-type>DisplayString</data-type>
		</vb>
		
		<vb>
			<oid>1.3.6.1.4.1.9.9.96.1.1.1.1.7.$index</oid>
			<value>$FTPUser</value>
			<data-type>DisplayString</data-type>	
		</vb>
		
		<vb>
			<oid>1.3.6.1.4.1.9.9.96.1.1.1.1.8.$index</oid>
			<value>$FTPPassword</value>
			<data-type>DisplayString</data-type>	
		</vb>
		
		<vb>
			<oid>1.3.6.1.4.1.522.3.4.5.0</oid>
			<value>2</value>	
			<error>Could not initiate transfer</error>
		</vb>
	</vb-list>	
		
</definition>