
<adapter name="CiscoSNMP">
	<description>Cisco Aironet access points, 340 and 350 series, VXWorks Software</description>
	<version>1.0.0</version>
	
	<services>
		<service name="ConfigBackup">		
		    <item type="builder_definition">CiscoSNMP_Config_Backup_Builder.xml</item>
		    <item type="snmp_script">CiscoSNMP_Config_Backup_Builder_Script.xml</item>			
		</service>

		<service name="ConfigDeploy">			
		    <item type="builder_definition">CiscoSNMP_Config_Deploy_Builder.xml</item>
		    <item type="snmp_script">CiscoSNMP_Config_Deploy_Builder_Script.xml</item>			
		</service>

		<!--service name="ImageDeploy">		
			<item type="builder_definition">CiscoSNMP_Image_Deploy_Builder.xml</item>
			<item type="snmp_script">CiscoSNMP_Image_Deploy_Builder_Script.xml</item>			
		</service-->

	</services>
</adapter>

