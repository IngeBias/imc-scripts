
<adapter name="JuniperRedline">
	<description>Juniper E|X DeviceType, 3650 Series, OS version 5.0</description>
	<version>1.0.0</version>
	<services>
		<service name="CLICommon">
			<item type="common">JuniperRedline_Common_CLI.xml</item>
		</service>

		<service name="CleanupParser">
			<item type="parser_definition">JuniperRedline_Cleanup_Parser.xml</item>
			<item type="perl_script">JuniperRedline_Cleanup_Parser_Script.pl</item>
		</service>

		<service name="ConfigBackup">
			<item type="builder_definition">JuniperRedline_Config_Backup_Builder.xml</item>
			<item type="tcl_script">JuniperRedline_Config_Backup_Builder_Script.xml</item>
		</service>

		<service name="ConfigDeploy">
			<item type="builder_definition">JuniperRedline_Config_Deploy_Builder.xml</item>
			<item type="tcl_script">JuniperRedline_Config_Deploy_Builder_Script.xml</item>
		</service>
		
		<service name="ImageDeploy">
			<item type="builder_definition">JuniperRedline_Image_Deploy_Builder.xml</item>
			<item type="tcl_script">JuniperRedline_Image_Deploy_Builder_Script.xml</item>
		</service>
	</services>
</adapter>

