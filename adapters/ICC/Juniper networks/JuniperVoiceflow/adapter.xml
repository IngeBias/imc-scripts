
<adapter name="JuniperVoiceflow">
	<description>JuniperVoiceflow series switch and router, include every oem device</description>
	<version>1.0.0</version>
	<services>
		<service name="CLICommon">
			<item type="common">JuniperVoiceflow_Common_CLI.xml</item>
		</service>

		<service name="CleanupParser">
			<item type="parser_definition">JuniperVoiceflow_Cleanup_Parser.xml</item>
			<item type="perl_script">JuniperVoiceflow_Cleanup_Parser_Script.pl</item>
		</service>

		<service name="ConfigBackup">
			<item type="builder_definition">JuniperVoiceflow_Config_Backup_Builder.xml</item>
			<item type="tcl_script">JuniperVoiceflow_Config_Backup_Builder_Script.xml</item>
		</service>

		<service name="ConfigDeploy">
			<item type="builder_definition">JuniperVoiceflow_Config_Deploy_Builder.xml</item>
			<item type="tcl_script">JuniperVoiceflow_Config_Deploy_Builder_Script.xml</item>
		</service>

		<service name="ImageDeploy">
			<item type="builder_definition">JuniperVoiceflow_Image_Deploy_Builder.xml</item>
			<item type="tcl_script">JuniperVoiceflow_Image_Deploy_Builder_Script.xml</item>
		</service>

	</services>
</adapter>

