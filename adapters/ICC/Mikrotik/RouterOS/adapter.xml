
<adapter name="RouterOS">
	<description>Mikrotik RouterOS-based systems</description>
	<version>1.0.0</version>
	<services>
		<service name="CLICommon">
			<item type="common">RouterOS_Common_CLI.xml</item>
		</service>

		<service name="CleanupParser">
			<item type="parser_definition">RouterOS_Cleanup_Parser.xml</item>
			<item type="perl_script">RouterOS_Cleanup_Parser_Script.pl</item>
		</service>

		<service name="ConfigBackup">
			<item type="builder_definition">RouterOS_Config_Backup_Builder.xml</item>
			<item type="tcl_script">RouterOS_Config_Backup_Builder_Script.xml</item>
		</service>

	</services>
</adapter>

