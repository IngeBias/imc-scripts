
<adapter name="BrocadeGeneric">
	<description>Brocade ICX</description>
	<version>1.0.0</version>
	<services>
		<service name="CLICommon">
			<item type="common">Brocade_Common_CLI.xml</item>
		</service>

		<service name="CleanupParser">
			<item type="parser_definition">Brocade_Cleanup_Parser.xml</item>
			<item type="perl_script">Brocade_Cleanup_Parser_Script.pl</item>
		</service>

		<service name="ConfigBackup">
			<item type="builder_definition">Brocade_Config_Backup_Builder.xml</item>
			<item type="tcl_script">Brocade_Config_Backup_Builder_Script.xml</item>
		</service>

	</services>
</adapter>

