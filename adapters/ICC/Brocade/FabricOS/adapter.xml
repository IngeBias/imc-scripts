
<adapter name="FabricOS">
	<description>FabricOS version 6.x</description>
	<version>1.0.0</version>
	<services>
		<service name="CLICommon">
			<item type="common">FabricOS_CLI.xml</item>
		</service>
		<service name="CleanupParser">
			<item type="parser_definition">FabricOS_Cleanup_Parser.xml</item>
			<item type="perl_script">FabricOS_Cleanup_Parser_Script.pl</item>
		</service>
		<service name="ConfigBackup">
			<item type="builder_definition">FabricOS_Config_Backup_Builder.xml</item>
			<item type="tcl_script">FabricOS_Config_Backup_Builder_Script.xml</item>
		</service>
	</services>
</adapter>
